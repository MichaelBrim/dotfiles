# NCCS/OLCF projects
apt ALIASTO 'modld mjb-proj/stf010 ; cdproj'
cdproj ALIASTO 'cd $MJB_PROJECT_DIR'
cdscratch ALIASTO 'cd $MJB_SCRATCH_DIR'
eash ALIASTO 'sudo /opt/eas/bin/eash'
supfa ALIASTO 'sudo su - pfaccept'
unifycr ALIASTO 'modld mjb-proj/unifycr ; cdproj'
unity ALIASTO 'modld mjb-proj/unity ; cdproj'

