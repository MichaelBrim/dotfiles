# misc mjb scripts
killvnc ALIASTO '$HOME/scripts/util/kill_VNC_server.sh home'
ldpth ALIASTO 'eval `$HOME/scripts/util/env_path_util.SHELLEXT LD_LIBRARY_PATH`'
ldadd ALIASTO 'eval `$HOME/scripts/util/env_path_util.SHELLEXT LD_LIBRARY_PATH add ARGV`'
ldrm ALIASTO 'eval `$HOME/scripts/util/env_path_util.SHELLEXT LD_LIBRARY_PATH rm ARGV`'
note ALIASTO '$HOME/scripts/util/note.sh'
pth ALIASTO 'eval `$HOME/scripts/util/env_path_util.SHELLEXT PATH`'
pthadd ALIASTO 'eval `$HOME/scripts/util/env_path_util.SHELLEXT PATH add ARGV`'
pthrm ALIASTO 'eval `$HOME/scripts/util/env_path_util.SHELLEXT PATH rm ARGV`'
sfvl ALIASTO '$HOME/scripts/software/snoflake/view-snoflake-log.bash -c'

# HPSS
hsils ALIASTO 'hsi ls'
hsilsr ALIASTO 'hsi ls -R'
hsill ALIASTO 'hsi ls -B'
hsillr ALIASTO 'hsi ls -BR'

