eos ALIASTO 'ssh eos-login2'
home ALIASTO 'ssh home'
hub ALIASTO 'ssh hub'
lyra ALIASTO 'ssh lyra-login1'
peak ALIASTO 'ssh peak.olcf.ornl.gov'
percival ALIASTO 'ssh percival.ccs.ornl.gov'
